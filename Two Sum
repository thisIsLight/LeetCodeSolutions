/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
    var numsHashmap = new Map();
    for(let i = 0; i < nums.length; i++){
        if(numsHashmap.has(target - nums[i]))
        {
            let result = [];
            result.push(numsHashmap.get(target - nums[i]))
            result.push(i);
            return result;
        }
        numsHashmap.set(nums[i],i);
    }
};
